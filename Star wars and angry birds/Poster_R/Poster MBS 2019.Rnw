library(lme4)
library(MASS)
library(xtable)
library(ggplot2)
library("texreg")
library(memisc)
library(dplyr)



choic <- read.csv("data/Choice_Tred.csv", header = TRUE)

choic$Btotal <- with(choic, BtoR + BtoB)


choic$PropR <- with(choic, 2*asin(sqrt(BtoR/Btotal)))

Mod_final <- lm(PropR ~ Dmorph * Rmorph, data = choic)
# xtable(coef(summary(Mod_final)))

@


