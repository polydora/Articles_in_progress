new_data2 <- choic %>% group_by(Dmorph, Rmorph) %>% do(data.frame(DL = mean(choic$DL), RL = mean(choic$RL)))
  

new_data2$fit_eta <- predict(ModN_glm5, newdata = new_data2)

new_data2$fit <- exp(new_data2$fit_eta)



X <- model.matrix(~ Dmorph + Rmorph + RL + DL + Dmorph:Rmorph + RL:DL , data = new_data2  )

new_data2$SE <- sqrt(diag(X %*% vcov(ModN_glm5) %*% t(X)))

new_data2$Upr <- exp(new_data2$fit_eta + 1.96*new_data2$SE)
new_data2$Lwr <- exp(new_data2$fit_eta - 1.96*new_data2$SE)


new_data2$signif <- c("a", " ", "a"," ")

levels(new_data2$Dmorph) <-  c("Донор E-морфотип", "Донор T-морфотип")


Pl_N <- ggplot(new_data2, aes(x = Rmorph, y = fit)) + geom_col(fill = "green", color = "black") +  geom_errorbar(aes(ymin = Lwr, ymax = Upr), width = 0.2)  + facet_wrap(~Dmorph) + theme_bw()  + geom_text(aes(label = signif, y = Upr+ 1), size = 6)+ labs(x = "Морфотип реципиента", y = "Количество нитей, пикрепленных \nк раковине реципиента " )+ theme(axis.text = element_text(size = 15), axis.title = element_text(size = 20), strip.text = element_text(size = 20))
ggsave("figures/ModN_visualisation.png", Pl_N)

